{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Install App Dependencies",
			"command": "pwsh",
			
			"args": [
				"-F","10-install-appdependencies.ps1"
			],
			"options": {
				"cwd": "${workspaceFolder}/10-setup"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"type": "shell",
			"label": "Install Web Dependencies",
			"command": "pwsh",
			"args": [
				"-F","10-install-webdependencies.ps1"
			],
			"options": {
				"cwd": "${workspaceFolder}/10-setup"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"type": "shell",
			"label": "Web Dev",
			"command": "pnpm",
			"args": [
				"dev"
			],
			"options": {
				"cwd": "${workspaceFolder}/.koksmat/web"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
	]
}